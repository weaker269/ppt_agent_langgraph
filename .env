# PPT Agent 环境配置

# OpenAI 配置
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_BASE_URL=https://api.openai.com/v1

# Google GenAI 配置
GOOGLE_API_KEY=your_google_api_key_here

# PPT 生成配置
PPT_QUALITY_THRESHOLD=85
MAX_RETRY_COUNT=3
CONCURRENT_SECTIONS=6

# 日志配置
LOG_LEVEL=INFO
LOG_FILE=logs/ppt_agent.log

# 输出配置
OUTPUT_DIR=results
HTML_TEMPLATE_DIR=templates